{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "DefinedTermSet",
  "type": "object",
  "definitions": {
    "DefinedTermSet": {
      "type": "object",
      "properties": {
        "hasDefinedTerm": {
          "title": "Has Defined Terms",
          "type": "array",
          "uniqueItems": true,
          "items": {
            "title": "Has Defined Term",
            "type": "string",
            "cordra": {
              "type": {
                "handleReference": {
                  "types": [
                    "DefinedTerm"
                  ]
                }
              }
            }
          }
        },
        "isPartOf": {
          "title": "Is Part Of",
          "type": "array",
          "uniqueItems": true,
          "items": {
            "title": "Is Part Of",
            "type": "string",
            "cordra": {
              "type": {
                "handleReference": {
                  "types": [
                    "Collection"
                  ]
                }
              }
            }
          }
        },
        "uploadDate": {
          "title": "Upload Date",
          "description": "A date value in ISO 8601 date format, e.g.: YYYY-MM-DD, YYYY-MM-DDThh:mm:ssTZD",
          "type": "string"
        }
      }
    }
  }
}
