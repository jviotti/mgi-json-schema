{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "DataDownload",
  "type": "object",
  "definitions": {
    "DataDownload": {
      "title": "Distribution",
      "type": "object",
      "properties": {
        "@type": {
          "default": "DataDownload",
          "type": "string"
        },
        "contentUrl": {
          "title": "Content URL",
          "type": "string"
        },
        "encodingFormat": {
          "type": "array",
          "items": {
            "title": "Data Type",
            "type": "string",
            "cordra": {
              "type": {
                "handleReference": {
                  "types": [
                    "DataFormat"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}
