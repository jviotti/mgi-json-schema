{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "QuantitativeValue",
  "type": "object",
  "definitions": {
    "QuantitativeValue": {
      "type": "object",
      "properties": {
        "@type": {
          "default": "QuantitativeValue",
          "type": "string"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "unitCode": {
          "title": "Unit Code",
          "type": "string",
          "cordra": {
            "type": {
              "handleReference": {
                "types": [
                  "UnitOfMeasurement"
                ]
              }
            }
          }
        },
        "unitText": {
          "title": "Unit Text",
          "type": "string"
        },
        "value": {
          "title": "Value"
        }
      }
    }
  }
}
