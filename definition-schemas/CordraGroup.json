{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "CordraGroup",
  "type": "object",
  "definitions": {
    "CordraGroup": {
      "type": "object",
      "properties": {
        "users": {
          "title": "Users",
          "type": "array",
          "format": "table",
          "uniqueItems": true,
          "items": {
            "title": "User",
            "type": "string",
            "cordra": {
              "type": {
                "handleReference": {
                  "types": [
                    "User",
                    "Group"
                  ]
                }
              }
            }
          },
          "cordra": {
            "auth": "usersList"
          }
        }
      }
    }
  }
}
